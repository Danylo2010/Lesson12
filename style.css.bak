@import url('https://fonts.googleapis.com/css2?family=Permanent+Marker&display=swap');
*{
	box-sizing: border-box;
	margin:0;
	padding:0;
}
.ANGRYBIRDS{ 
 height: 600px;
 border: 1px solid red;
 background-image: url("./images/front_sky.png"), url("./images/middle_sky.png"), url("./images/back_sky.png"),
 url("./images/under_back_sky.png"), url("./images/front_grass.png"),url("./images/middle_grass.png"),url("./images/back_grass.png"),
 linear-gradient(180deg, rgba(218,214,21,0.5816432119205298) 1%, rgba(255,209,0,1) 100%);;
 background-repeat:no-repeat;
 background-size: 100%;
 background-position: top,top,top,top,bottom, bottom, bottom;
  linear-gradient(180deg, rgba(218,148,21,0.9856167218543046) 1%, rgba(255,209,0,1) 100%);
 font-family: "Permanent Marker", cursive;
 font-size:80px;
 text-align:center;
 padding-top:30px;
}	
.title{
color:red;	
text-shadow:10px 10px 10px #333;	
}